{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build with gcc 13",
			"command": "/home/linuxbrew/.linuxbrew/bin/g++-13",
			"args": [
				"-g",
				"-std=c++20",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${fileDirname}/rooster"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compilador: /home/linuxbrew/.linuxbrew/bin/g++-13"
		},
		{
			"type": "cppbuild",
			"label": "Build with clang++14.0.6",
			"command": "/usr/bin/clang++-14",
			"args": [
				"-g",
				"-std=c++20",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${fileDirname}/rooster"
			
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compilador: /usr/bin/clang++-14"
		}
	]
}